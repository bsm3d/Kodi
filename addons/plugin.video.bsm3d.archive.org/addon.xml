<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.bsm3d.archive.org" name="Internet Archive Browser" version="1.0.0" provider-name="BSM3D">
  <requires>
    <import addon="xbmc.python" version="3.0.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>audio video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en_GB">Internet Archive Browser add-on</summary>
    <description lang="en_GB">Watch and listen to media archived by Internet Archive</description>
    <disclaimer lang="en_GB">This third party addon is not in any way commissioned or endorsed by Internet Archive</disclaimer>
    <language></language>
    <platform>all</platform>
    <license>GPL-2.0</license>
    <website>https://archive.org/</website>
    <assets>
      <icon>icon.png</icon>
      <fanart>fanart.jpg</fanart>
    </assets>
  </extension>
</addon>